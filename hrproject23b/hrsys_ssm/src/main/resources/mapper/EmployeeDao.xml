<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lz08.hrsys_ssm.dao.EmployeeDao">
    <select id="search" resultType="com.lz08.hrsys_ssm.entity.Employee">
        select * from employee
    </select>

    <select id="searchById" resultType="com.lz08.hrsys_ssm.entity.Employee">
        select * from employee where id=#{id}
    </select>

</mapper>
